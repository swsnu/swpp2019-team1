  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "react-scripts test",
    "installa": "yarn install && cd ../back && pip install -r requirements.txt",
    "installf": "yarn install",
    "installb": "cd ../back && pip install -r requirements.txt",
    "saveb": "../test.sh saveb",
    "testf_": "react-scripts test --coverage --watchAll=false",
    "lintf_": "eslint src",
    "testf": "../test.sh -f",
    "testb": "../test.sh -b",
    "testa": "../test.sh -a",
    "testfl": "../test.sh -fl",
    "testbl": "../test.sh -bl",
    "testal": "../test.sh -al",
    "lintf": "../test.sh -l",
    "lintb": "cd ../back && ../test.sh -l",
    "linta": "cd ../ && ./test.sh -l",
    "back": "python3 ../back/manage.py runserver 8000",
    "dev": "concurrently --kill-others \"yarn back\" \"yarn start\"",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
